<?xml version="1.0" encoding="UTF-8"?>
<dispatch-entries>
  <dispatch>
      <!-- Run tasks in the background module. -->
      <url>*/tasks</url>
      <module>background</module>
  </dispatch>
  <dispatch>
      <!-- Used to verify that dispatch.xml works. -->
      <url>*/admin/module</url>
      <module>background</module>
  </dispatch>
  <dispatch>
      <!-- Sentry task queue -->
      <url>*/_ah/queue/__deferred__</url>
      <module>background</module>
  </dispatch>
</dispatch-entries>