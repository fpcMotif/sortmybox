<?xml version="1.0" encoding="UTF-8"?>
<cronentries>
    <cron>
        <url>/cron/rules/RuleProcessor</url>
        <description>Runs RuleProcessor job</description>
        <schedule>every 15 minutes</schedule>
    </cron>
    <cron>
        <url>/cron/cron/FileMoveSweeper</url>
        <description>Runs FileMoveSweeper</description>
        <schedule>every day 3:00</schedule>
        <timezone>America/Los_Angeles</timezone>
    </cron>
    <cron>
        <url>/cron/cron/UsageStatsGatherer</url>
        <description>Runs UsageStatsGatherer</description>
        <schedule>every day 1:00</schedule>
        <timezone>America/Los_Angeles</timezone>
    </cron>
    <cron>
        <url>/cron/cron/DailyUsageStatsGatherer</url>
        <description>Runs DailyUsageStatsGatherer</description>
        <schedule>every day 1:30</schedule>
        <timezone>America/Los_Angeles</timezone>
    </cron>
</cronentries>